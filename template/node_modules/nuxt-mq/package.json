{
  "name": "nuxt-mq",
  "version": "1.1.3",
  "description": "vue-mq module for Nuxt.js",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "nuxt.js",
    "nuxtjs",
    "vue",
    "vue.js",
    "vuejs",
    "media queries"
  ],
  "contributors": [
    {
      "name": "Maarten Van Hoof (@mrtnvh)"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/vanhoofmaarten/nuxt-mq",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture -c with_mq_nuxt.config.js",
    "dev-debug": "node --inspect ./node_modules/nuxt/bin/nuxt test/fixture -c with_mq_nuxt.config.js",
    "build": "nuxt build test/fixture -c with_mq_nuxt.config.js",
    "start": "nuxt start test/fixture -c with_mq_nuxt.config.js",
    "generate": "nuxt generate test/fixture -c with_mq_nuxt.config.js",
    "build-spa": "nuxt build test/fixture -c with_mq_nuxt.config.js --spa  --verbose",
    "start-spa": "nuxt start test/fixture -c with_mq_nuxt.config.js --spa",
    "lint": "eslint lib test",
    "test": "npm run lint && jest --detectOpenHandles",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "vue-mq": "latest"
  },
  "devDependencies": {
    "codecov": "latest",
    "ecstatic": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "nuxt": "latest",
    "pkg-dir": "latest",
    "promisify-child-process": "^2.1.2",
    "standard-version": "latest"
  }
}
